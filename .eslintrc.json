{
    "extends": ["airbnb"],
    "plugins": [
        "react",
        "jsx-a11y"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "es6": true,
        "jest": true,
        "browser": true,
        "node": true
    }
}